package models

import (
	github.com
)

type struct UserBasic{
	gorm.Model
	Name string
	PassWord string
	Phone string
	Email string
	Identity string
	ClientIp string
	ClientPort string
	LoginTime uint64
	heatBeatTime uint64
	LogOutTime uint64
	isLoginOut bool
	DeviceInfo string
}

func (table *UserBasic) TableName() string{
	return "user_basic"
}